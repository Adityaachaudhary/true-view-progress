<svg xmlns="http://www.w3.org/2000/svg" width="800" height="400" viewBox="0 0 800 400">
  <rect width="100%" height="100%" fill="#1a1a2e"/>
  
  <!-- Analytics Modal -->
  <rect x="200" y="50" width="400" height="300" fill="#2a2a3e" rx="8"/>
  
  <!-- Modal Header -->
  <text x="220" y="90" fill="white" font-family="Arial" font-size="20">Viewing Analytics</text>
  <text x="220" y="115" fill="#8a8aa3" font-family="Arial" font-size="14">Detailed breakdown of your viewing progress</text>
  
  <!-- Watched Segments Section -->
  <text x="220" y="150" fill="white" font-family="Arial" font-size="16">Watched Segments</text>
  <text x="220" y="180" fill="#8a8aa3" font-family="Arial" font-size="14">1:25 - 1:39</text>
  <text x="500" y="180" fill="#8a8aa3" font-family="Arial" font-size="14">0:13</text>
  
  <!-- Progress Stats -->
  <text x="220" y="220" fill="white" font-family="Arial" font-size="14">Total Unique Time:</text>
  <text x="500" y="220" fill="white" font-family="Arial" font-size="14">0:13</text>
  
  <text x="220" y="250" fill="white" font-family="Arial" font-size="14">Video Duration:</text>
  <text x="500" y="250" fill="white" font-family="Arial" font-size="14">9:56</text>
  
  <text x="220" y="280" fill="white" font-family="Arial" font-size="14">Progress:</text>
  <text x="500" y="280" fill="white" font-family="Arial" font-size="14">2.3%</text>
  
  <!-- Action Buttons -->
  <rect x="220" y="310" width="80" height="30" fill="#4a4a5e" rx="4"/>
  <text x="260" y="330" fill="white" font-family="Arial" font-size="12" text-anchor="middle">Export</text>
  
  <rect x="310" y="310" width="80" height="30" fill="#4a4a5e" rx="4"/>
  <text x="350" y="330" fill="white" font-family="Arial" font-size="12" text-anchor="middle">Import</text>
</svg>